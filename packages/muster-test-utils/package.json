    "build:dist": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json"
  },
  "author": "DWS Technology",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "typescript": "3.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
