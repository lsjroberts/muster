{
  "name": "@dws/muster-test-utils",
  "version": "6.5.0",
  "description": "Muster Test Utils",
  "module": "./esm/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "scripts": {
