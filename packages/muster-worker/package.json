{
  "name": "@dws/muster-worker",
  "version": "6.5.0",
  "description": "Muster Worker",
  "module": "./esm/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "scripts": {
    "clean": "del-cli ./dist ./esm",
    "prebuild": "yarn clean",
    "prebuild:full": "yarn clean",
    "build": "yarn build:dist",
    "build:full": "yarn build:dist && yarn build:esm",
    "build:dist": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "webpack-dev-server -d"
  },
  "author": "DWS Technology",
  "license": "MIT",
  "dependencies": {
    "@dws/muster": "^6.5.0",
    "@dws/muster-message-transport": "^6.5.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.14.74",
    "ts-loader": "^5.2.2",
    "typescript": "3.1.1",
    "webpack": "^4.19.0",
    "webpack-dev-server": "^3.1.8"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
